<service>
	<id>omniserver.exe</id>
	<name>Omniserver</name>
	<description>Omniserver.</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>D:\OMNI\LaundryServer\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\OMNI\LaundryServer\app.js</argument>
	<argument>--log</argument>
	<argument>Omniserver wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\OMNI\LaundryServer\WinService\Install</workingdirectory>
</service>